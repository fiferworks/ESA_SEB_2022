@article{Rwomushana2017,
  title = {Detection of {{Diaphorina}} Citri {{Kuwayama}} ({{Hemiptera}}: {{Liviidae}}) in {{Kenya}} and Potential Implication for the Spread of {{Huanglongbing}} Disease in {{East Africa}}},
  shorttitle = {Detection of {{Diaphorina}} Citri {{Kuwayama}} ({{Hemiptera}}},
  author = {Rwomushana, Ivan and Khamis, Fathiya M. and Grout, Tim G. and Mohamed, Samira A. and Sétamou, Mamoudou and Borgemeister, Christian and Heya, Hellen M. and Tanga, Chrysantus M. and Nderitu, Peterson W. and Seguni, Zuberi S. and Materu, Christopher L. and Ekesi, Sunday},
