@article{Om2017,
  title = {A New Species of {{Tamarixia Mercet}} ({{Hymenoptera}}, {{Eulophidae}}), a Parasitoid of {{Diaphorina}} Communis {{Mathur}} ({{Hemiptera}}, {{Liviidae}}) in {{Bhutan}}},
  author = {Om, Namgay and Yefremova, Zoya A. and Yegorenkova, Ekaterina N. and Beattie, G. Andrew C. and Donovan, Nerida J. and Holford, Paul},
