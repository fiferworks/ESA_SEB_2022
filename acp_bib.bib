  author = {Sarmento, Renato Almeida and Lemos, Felipe and Bleeker, Petra M. and Schuurink, Robert C. and Pallini, Angelo and Oliveira, Maria Goreti Almeida and Lima, Eraldo R. and Kant, Merijn and Sabelis, Maurice W. and Janssen, Arne},
