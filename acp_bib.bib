@article{Hall2009,
  title = {An {{Assessment}} of {{Yellow Sticky Card Traps}} as {{Indicators}} of the {{Abundance}} of {{Adult}} {{{\emph{Diaphorina}}}}{\emph{ Citri}} ({{Hemiptera}}: {{Psyllidae}}) in {{Citrus}}},
  shorttitle = {An {{Assessment}} of {{Yellow Sticky Card Traps}} as {{Indicators}} of the {{Abundance}} of {{Adult}} {{{\emph{Diaphorina}}}}{\emph{ Citri}} ({{Hemiptera}}},
  author = {Hall, David G.},
