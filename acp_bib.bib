  shorttitle = {Predatory {{Mite}}, {{{\emph{Amblyseius}}}}{\emph{ Swirskii}} ({{Acari}}},
  author = {Juan-Blasco, María and Qureshi, Jawwad A. and Urbaneja, Alberto and Stansly, Philip A.},
  date = {2012-09},
