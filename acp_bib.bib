  shorttitle = {Disruption of the {{Leafminer}} {{{\emph{Phyllocnistis}}}}{\emph{ Citrella}} ({{Lepidoptera}}},
  author = {Lapointe, S. L. and Keathley, Craig P. and Stelinski, L. L. and Urrutia, W. H. and Mafra-Neto, A.},
