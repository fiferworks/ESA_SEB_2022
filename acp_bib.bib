@article{Hort2014,
  title = {Selektivitas Pestisida Terhadap Perkembangan Cendawan Entomopatogen Hirsutella citriformis (Compatibility Between Pesticides With Fungus Entomopathogen of Hirsutella citriformis Development)},
  author = {Hort, J},
