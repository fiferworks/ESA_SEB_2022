@article{Nakamoto2002,
  title = {"A newly developed LED (Light Emitting Diode) trap for the West Indian sweet potato weevil, Euscepes postfasciatus (Fairmaire) (Coleoptera: Curculionidae)."},
  shorttitle = {"A newly developed LED (Light Emitting Diode) trap for the West Indian sweet potato weevil, Euscepes postfasciatus (Fairmaire) (Coleoptera},
  author = {Nakamoto, Y. and Takushi, J.},
  date = {2002},
  journaltitle = {Japanese Journal of Applied Entomology and Zoology (Japan)},
  issn = {0021-4914},
  doi = {10.1303/jjaez.2002.145},
  url = {https://scholar.google.com/scholar_lookup?title=%22A+newly+developed+LED+%28Light+Emitting+Diode%29+trap+for+the+West+Indian+sweet+potato+weevil%2C+Euscepes+postfasciatus+%28Fairmaire%29+%28Coleoptera%3A+Curculionidae%29.%22&author=%22Nakamoto%2C+Y.+%28Okinawa-ken.+Government+Office%2C+Naha+%28Japan%29%29&publication_year=2002},
  urldate = {2022-03-07},
  abstract = {"We confirmed positive photo tactic responses of the West Indian sweet potato weevil, Euscepes postfasciatus (Fairmaire) (Coleoptera: Curculionidae) to red, yellow, green, and blue rays of a LED (light emitting diode) in the laboratory. We examined a trap baited with sweet potato roots and the LED sources for attraction of the weevils in a sweet potato field. Except for red LED, the sweet potato root traps with the yellow, green or blue LED captured significantly more weevils than that without the LED source in the field. Furthermore, we developed a new LED trap without sweet potato roots, and confirmed that the trap was applicable for efficient capture of weevils in a mature sweet potato field."},
  langid = {japanese}
}

@article{Nakamoto2004,
