@article{Kubota2020,
  title = {Perilla Mosaic Virus Is a Highly Divergent Emaravirus Transmitted by {{{\emph{Shevtchenkella}}}} Sp. ({{Acari}}: {{Eriophyidae}})},
  author = {Kubota, Kenji and Usugi, Tomio and Tomitaka, Yasuhiro and Shimomoto, Yoshifumi and Takeuchi, Shigeharu and Kadono, Fujio and Yanagisawa, Hironobu and Chiaki, Yuya and Tsuda, Shinya},
