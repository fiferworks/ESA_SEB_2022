@article{Belliure2010,
  title = {Vector and Virus Induce Plant Responses That Benefit a Non-Vector Herbivore},
  author = {Belliure, Belén and Sabelis, Maurice W. and Janssen, Arne},
  date = {2010-03},
  journaltitle = {Basic and Applied Ecology},
  volume = {11},
