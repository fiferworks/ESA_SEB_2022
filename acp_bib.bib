@article{Garcia-Escamilla2018,
  title = {Transmission of Viruses Associated with Cytoplasmic and Nuclear Leprosis Symptoms by {{{\emph{Brevipalpus}}}}{\emph{ Yothersi}} and {{{\emph{B}}}}{\emph{. Californicus}}},
  author = {García-Escamilla, Paul and Duran-Trujillo, Yuridia and Otero-Colina, Gabriel and Valdovinos-Ponce, Guadalupe and Santillán-Galicia, Ma. Teresa and Ortiz-García, Carlos F. and Velázquez-Monreal, José J. and Sánchez-Soto, Saúl},
