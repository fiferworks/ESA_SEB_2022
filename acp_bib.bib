@article{Ramakrishnan2015,
  title = {Eco-Safe Biopesticides to Manage Psyllids, {{{\emph{Diaphorina}}}}{\emph{ Citri}} in Curry Leaf, {{{\emph{Murraya}}}}{\emph{ Koenigii}}},
