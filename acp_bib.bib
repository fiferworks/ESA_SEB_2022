@article{Martinez-Turino2021,
  title = {Virus Host Jumping Can Be Boosted by Adaptation to a Bridge Plant Species},
  author = {Martínez-Turiño, Sandra and Calvo, María and Bedoya, Leonor Cecilia and Zhao, Mingmin and García, Juan Antonio},
  date = {2021-04},
  journaltitle = {Microorganisms},
