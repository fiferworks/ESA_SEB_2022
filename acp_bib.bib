@article{Hung2001,
  title = {Identification of the {{Chinese Box Orange}} ({{{\emph{Severinia}}}}{\emph{ Buxifolia}}) as an Alternative Host of the Bacterium Causing Citrus {{Huanglongbing}}},
  author = {Hung, Ting-Hsuan and Wu, Meng-Ling and Su, Hong-Ji},
