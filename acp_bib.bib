@article{Fadamiro2008,
  title = {Seasonal {{Occurrence}} of {{Key Arthropod Pests}} and {{Associated Natural Enemies}} in {{Alabama Satsuma Citrus}}},
  author = {Fadamiro, Henry Y and Xiao, Yingfang and Hargroder, Terry and Nesbitt, Monte and Umeh, Vincent and Childers, Carl C},
