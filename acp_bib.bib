@article{Garita2014,
  title = {Experimental Host Range of {{Citrus}} Leprosis Virus {{C}} ({{CiLV-C}})},
  author = {Garita, Laura Cristina and Tassi, Aline Daniele and Calegario, Renata Faier and Freitas-Astúa, Juliana and Salaroli, Renato B. and Romão, Gerson O. and Kitajima, Elliot W.},
