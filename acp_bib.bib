@article{Kim2019,
  title = {Advances in Insect Phototaxis and Application to Pest Management: A Review},
  shorttitle = {Advances in Insect Phototaxis and Application to Pest Management},
  author = {Kim, Kil-Nam and Huang, Qiu-Ying and Lei, Chao-Liang},
  date = {2019},
  journaltitle = {Pest Management Science},
