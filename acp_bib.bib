@article{Oliver1971,
  title = {Parthenogenesis in Mites and Ticks ({{Arachnida}}: {{Acari}})},
  author = {Oliver, James H.},
  date = {1971-05},
  journaltitle = {American Zoologist},
  volume = {11},
