@article{Mendonca2011,
  title = {Tabela de Vida de Fertilidade de {{{\emph{Brevipalpus}}}}{\emph{ Phoenicis}} ({{Geijskes}}, 1939) ({{Acari}}: {{Tenuipalpidae}}) Em Diferentes Cultivares de Café ({{Coffea}} Spp.)},
