@article{Wendel2022,
  title = {Screening Commercial Entomopathogenic Fungi for the Management of {{{\emph{Diaphorina}}}}{\emph{ Citri}} Populations in the {{Lower Rio Grande Valley}}, {{Texas}}, {{USA}}},
  author = {Wendel, Justin and Cisneros, Jonathan and Jaronski, Stefan and Vitek, Christopher and Ciomperlik, Matthew and Flores, Daniel},
  date = {2022-01-22},
