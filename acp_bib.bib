@article{Halbert2004,
  title = {{{ASIAN CITRUS PSYLLIDS}} ({{STERNORRHYNCHA}}: {{PSYLLIDAE}}) {{AND GREENING DISEASE OF CITRUS}}: {{A LITERATURE REVIEW AND ASSESSMENT OF RISK IN FLORIDA}}},
  shorttitle = {{{ASIAN CITRUS PSYLLIDS}} ({{STERNORRHYNCHA}}},
