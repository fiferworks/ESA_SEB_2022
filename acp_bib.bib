@article{Warabieda2020,
  title = {Mutual Relations between Jasmonic Acid and Acibenzolar-{{S-methyl}} in the Induction of Resistance to the Two-Spotted Spider Mite ({{{\emph{Tetranychus}}}}{\emph{ Urticae}}) in Apple Trees},
  author = {Warabieda, Wojciech and Markiewicz, M. and Wójcik, D.},
