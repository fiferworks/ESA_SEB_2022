@article{Gadino2012,
  title = {Evaluation of Methyl Salicylate Lures on Populations of {{{\emph{Typhlodromus}}}}{\emph{ Pyri}} ({{Acari}}: {{Phytoseiidae}}) and Other Natural Enemies in Western {{Oregon}} Vineyards},
  author = {Gadino, Angela N. and Walton, Vaughn M. and Lee, Jana C.},
