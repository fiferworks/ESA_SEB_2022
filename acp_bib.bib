@article{Ramos-Gonzalez2017,
  title = {Citrus Leprosis Virus {{N}}: A New Dichorhavirus Causing {{Citrus}} Leprosis Disease},
  author = {Ramos-González, Pedro Luis and Chabi-Jesus, Camila and Guerra-Peraza, Orlene and Tassi, Aline Daniele and Kitajima, Elliot Watanabe and Harakava, Ricardo and Salaroli, Renato Barbosa and Freitas-Astúa, Juliana},
  date = {2017-08},
