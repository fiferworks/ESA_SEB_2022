@article{Bove2006,
  title = {Huanglongbing: A Destructive, Newly-Emerging, Century-Old Disease of Citrus},
  shorttitle = {{{HUANGLONGBING}}},
  author = {Bové, J.M.},
  date = {2006},
  journaltitle = {Journal of Plant Pathology},
  volume = {88},
  number = {1},
  eprint = {41998278},
