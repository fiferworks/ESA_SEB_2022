@article{Adair1998,
  title = {Ovipositional {{Preferences}} of {{Diaprepes}} Abbreviatus ({{Coleoptera}}: {{Curculionidae}})},
  shorttitle = {Ovipositional {{Preferences}} of {{Diaprepes}} Abbreviatus ({{Coleoptera}}},
  author = {Adair, R. C. and Nigg, H. N. and Simpson, S. E. and Lefevre, L.},
