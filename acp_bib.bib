@article{Amoros2018,
  title = {Response of {{{\emph{Diaphorina}}}}{\emph{ Citri}} ({{Hemiptera}}: {{Liviidae}}) to Volatiles Characteristic of Preferred Citrus Hosts},
  author = {Amorós, María Eugenia and das Neves, Virginia Pereira and Rivas, Fernando and Buenahora, José and Martini, Xavier and Stelinski, Lukasz L. and Rossini, Carmen},
  options = {useprefix=true},
